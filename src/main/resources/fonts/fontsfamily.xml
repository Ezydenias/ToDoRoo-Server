<?xml version="1.0" encoding="UTF-8"?>
<fontFamilies>
    <fontFamily name="Courgette">
        <!-- Normal Arial Font -->
        <normal>
            <![CDATA[fonts/Courgette/Courgette.ttf]]></normal> <!-- location of the nornal font on resource folder -->
        <!-- Bold Arial Font -->
        <bold><![CDATA[fonts/Courgette/Courgette.ttf]]></bold>
        <!-- Italic Arial Font -->
        <italic><![CDATA[fonts/Courgette/Courgette.ttf]]></italic>
        <!-- Bolt italic Arial Font -->
        <boldItalic><![CDATA[fonts/Courgette/Courgette.ttf]]></boldItalic>
